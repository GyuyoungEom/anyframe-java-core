<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="dashboard">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-dashboard-pi</artifactId>
  <version>1.0.0.RC1</version>
  <dependent-plugins>
    <dependent-plugin name="core" version="2.0.0&gt;*"/>
  </dependent-plugins>
  <resources>
    <resource dir="src/main/resources" filtered="true" packaged="false">
      <include name="**/*.xml"/>
    </resource>
    <resource dir="src/main/webapp" filtered="false" packaged="false">
      <include name="**/dashboard/**/*.*"/>
    </resource>
  </resources>
  <build>
    <filesets>
      <fileset dir="src/main/resources" filtered="true" packaged="false">
        <include name="**/**/*dashboard*.xml"/>
      </fileset>
      <fileset dir="src/main/webapp" filtered="false" packaged="false">
        <include name="**/dashboard/**/*.*"/>
      </fileset>
    </filesets>
  </build>
  <samples>true</samples>
  <interceptor>
    <class></class>
  </interceptor>
  <essential>false</essential>
</plugin>