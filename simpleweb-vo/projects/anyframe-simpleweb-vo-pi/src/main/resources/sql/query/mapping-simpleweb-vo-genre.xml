<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE queryservice PUBLIC "-//ANYFRAME//DTD QUERYSERVICE//EN"
"http://www.anyframejava.org/dtd/anyframe-core-query-mapping-3.2.dtd">
<queryservice>
	<queries>		
		<query id="findVoGenreList" isDynamic="false">
			<statement>
				SELECT genre_id, name
				FROM VO_GENRE
				ORDER BY name
			</statement>
			<result class="org.anyframe.plugin.simpleweb.vo.domain.Genre" />
		</query>		
	</queries>
</queryservice>

