<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="jasperreports" description="JasperReports Plugin consists of sample code that provides guidelines on how to execute Reporting by connecting Spring MVC and JasperReports, and reference libraries needed to use these open sources.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-jasperreports-pi</artifactId>
  <version>1.0.1</version>
  <dependent-plugins>
    <dependent-plugin name="core" version="2.0.0&gt;*"/>
  </dependent-plugins>
  <resources>
    <resource dir="src/main/java" filtered="true" packaged="true">
      <include name="**/*.java"/>
    </resource>
    <resource dir="src/main/resources" filtered="true" packaged="false">
      <include name="**/*.xml"/>
      <include name="**/*jasperreports*/*.*"/>
    </resource>
    <resource dir="src/main/webapp/WEB-INF/jsp" filtered="false" packaged="false">
      <include name="**/*.jsp"/>
      <include name="**/*.jasper"/>
      <include name="**/*.jrxml"/>
    </resource>
  </resources>
  <build>
    <filesets>
      <fileset dir="src/main/java" filtered="true" packaged="true">
        <include name="**/org/anyframe/plugin/jasperreports/**/*.java"/>
      </fileset>
      <fileset dir="src/main/resources" filtered="true" packaged="false">
      	<include name="**/jasperreports/*.*"/>
        <include name="**/*jasperreports*.xml"/>
      </fileset>
      <fileset dir="src/main/webapp" filtered="false" packaged="false">
        <include name="**/jasperreports/**/*.jasper"/>
        <include name="**/jasperreports/**/*.jrxml"/>
        <include name="**/web.xml"/>
        <include name="**/anyframe.jsp"/>
      </fileset>
    </filesets>
  </build>
  <samples>true</samples>
  <interceptor>
    <class></class>
  </interceptor>
  <essential>false</essential>
  <checked>false</checked>
</plugin>