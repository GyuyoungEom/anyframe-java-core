<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="cxf" description="CXF Plugin consists of reference libraries needed to use Apache CXF open source support for developing web service.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-cxf-pi</artifactId>
  <version>1.0.3</version>
  <dependent-plugins>
    <dependent-plugin name="core" version="2.0.0&gt;=*"/>
  </dependent-plugins>
  <resources>
  </resources>
  <build>
    <filesets>
    	<fileset dir="src/main/resources" filtered="true" packaged="false">
	       <include name="**/log4j.xml"/>
	    </fileset>
    </filesets>
  </build>
  <interceptor>
    <class></class>
  </interceptor>
  <samples>false</samples>
  <essential>false</essential>
  <checked>false</checked>
</plugin>